{
    "schema": {
        "rss": {
            "type": "object",
            "title": "RSS Settings",
            "properties": {
                "feeds": {
                    "type": "tabarray",
                    "title": "RSS Feeds URL",
                    "items": {
                        "type": "url"
                    }
                },
                "refreshInterval": {
                    "type": "integer",
                    "title": "Refresh Interval for RSS Feeds (minutes)",
                    "default": 10
                },
				"newsType": {
					"type": "string",
					"title": "News Type",
					"default": "TOPIC"
				},
				"newsTypeTerms": {
					"type": "string",
					"title": "News Type Terms",
					"default": "HEADLINES"
				}
            }
        }
    },
    "form": [
        {
            "type": "fieldset",
            "key": "rss",
            "expandable": true,
            "order":0
        }
    ]
}